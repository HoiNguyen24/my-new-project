<!DOCTYPE html>
<html lang="en">
<head>
    <meta charset="UTF-8">
    <title>Title</title>
    <style>
        #table {
            
            border-collapse: collapse;
        }
        #table tr td {
            border: 1px solid;
        }
    </style>
</head>
<body>

</body>
</html>
<script>
    let sout = "<table id='table'>";
    for(let i = 1 ; i < 10 ; i++){
        sout += "<tr><td>";
        for(let j = 2 ; j < 10 ; j++){
            sout += i +"x"+ j +"=" + i*j + "<td>";
        }
        sout += "</tr>";
    }
    sout += "</table>";
    document.write(sout);
</script>
