function bai3(a){
    let S = 1;
    for(var i = 1; i<= a;i++){
        S *= i;
    }
    return S;
}
function bai4(x){
    x = parseInt(x, 10);
    if( x <= 9 && x >= 0) return true;
    else return false;

}
function bai5 (a,b,c){
    min = a;
    if( b < min) min = b;
    if( c < min) min = c;
    return min;
}
