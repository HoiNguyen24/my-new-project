function isPrime(a){
    if(a < 2) return false;
    for(var i = 2; i <= Math.sqrt(a); i++){
        if(a % i == 0 ) return false;
    }
    return true;
}
function isPalandrome(a){
    let b = a.reverse();
    if( b === a) aleart("is Palandrome");
    else aleart("is not palandrome");
}
