function bai8 (a){
     let l= 0;
     let r = a.length-1;
     while(l< r){
         let temp = a[l];
         a[l] = a[r];
         a[r] = temp;
         l++;
         r--;
     }
     return a;
}
function bai9(a,b){
    let count = 0;
    for(let i = 0; i < a.length;i++){
        if(a[i] == b) count++;
    }
    if(count > 0) return count;
    else return -1;
}
